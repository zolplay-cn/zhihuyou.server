{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "setupFiles": ["./e2e-setup.ts"],
  "moduleNameMapper": {
    "~/(.*)": "<rootDir>/../src/$1",
    "test/(.*)": "<rootDir>/$1"
  }
}
